    "@angular/fire": "6.0.0",
    "@angular/forms": "^8.0.0 || ^9.0.0-0",
    "@angular/material": "8.2.3",
    "@angular/pwa": "12.1.4",
    "active-win": "8.1.0",
    "auto-launch": "5.0.6",
    "circular-json": "0.5.9",
    "electron-browser-storage": "1.0.7",
    "electron-localshortcut": "3.2.1",
    "electron-updater": "6.1.1",
